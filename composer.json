{
    "name": "rbertolli/tuya-php",
    "description": "Cliente PHP para a API Tuya",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "ext-curl": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.8",
        "psr/cache": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tuya\\Core\\": "src/Core/",
            "Tuya\\Laravel\\": "src/Laravel/",
            "Tuya\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tuya\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tuya\\Laravel\\TuyaServiceProvider"
            ]
        }
    }
}